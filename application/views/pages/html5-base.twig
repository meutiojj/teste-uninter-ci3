<!DOCTYPE html>
<html lang="pt-br">
<head>
<meta charset="UTF-8">
<base href="">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

{% block css_eager %}
<!-- html5.js for IE less than 9 -->
<!--[if lt IE 9]>
	<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->

<!-- css3-mediaqueries.js for IE less than 9 -->
<!--[if lt IE 9]>
	<script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
<![endif]-->
<link href="{{ bootstrap_url('dist/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
{% endblock%}
{% block js_eager %} 
<script src="{{ bootstrap_url('assets/js/vendor/jquery-slim.min.js') }}" ></script>
<script src="{{ bootstrap_url('dist/js/bootstrap.min.js') }}" ></script>
<script src="{{ bootstrap_url('assets/js/vendor/popper.min.js') }}" ></script>
{% endblock%} 
<title>{% block title %}{{pageTitle}}{% endblock %}</title>
</head>
<body>
    <div>
    {% block body %}
        {% block content %}
            {{content}}
        {% endblock %}       
    {% endblock %}
        <footer>
        	{%block footer %}        		
                {% block js_lazy %}                        
                {% endblock%}                 
                {% block css_lazy %}                                           
                {% endblock%} 
            {%endblock%}
        </footer>   
    </div>
</body>
</html>